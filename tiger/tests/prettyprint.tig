type tree = {key: string, left: tree, right: tree}

function prettyprint(tree: tree): string =
    let var output := ""
        function write(s: string) =
            output := concat(output, s)
        function show(n: int, t: tree) =
            let function indent(s: string) =
                (for i := 0 to n
                    do write(" ");
                    output := concat(output, s);
                    write("\n"))
            in
                if t = nil then
                    indent
                else (
                    indent(t.key);
                    show(n + 1, t.left);
                    show(n + 1, t.right)
                )
            end
    in
        show(0, tree);
        output
    end
