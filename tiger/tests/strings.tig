(
    printi(ord("a"), function() =
        print(chr(97), function() =
            print("\n", function() =
                print(chr(ord("a")), function() =
                    print("\n", function() =
                        if "a" = "a" then
                            print("true\n", function() =
                                getchar(function(char: string) =
                                    print(char, function() =
                                        print("\n", exit)
                                    )
                                )
                            )
                        else
                            exit()
                    )
                )
            )
        )
    )
)
