let var n := 10
    var output := concat("Hello, ", "world\n")
    function test3() = (
        print(output);
        output := concat("test3 ", output);
        printi(n)
    )
    function test() =
        let var s := 12
            function test2() = (
                print(output);
                test3();
                output := concat("test2 ", output);
                printi(n);
                printi(s)
            )
        in
            output := concat("test ", output);
            printi(n);
            test2()
        end
in
    test()
end
